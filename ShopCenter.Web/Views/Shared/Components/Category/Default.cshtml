@model IEnumerable<string>


<div class="container">
    <ul>
        <li>
            <a asp-controller="@ViewData["controller"]" asp-action="Index">همه</a>
        </li>
        @foreach (string item in Model)
        {
            <li>
                <a asp-controller="@ViewData["controller"]" asp-action="Index" asp-route-Category="@item">
                    @item
                </a>
            </li>
        }
    </ul>
</div>